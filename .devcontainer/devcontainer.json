{
  "name": "Alchemy QL Devenv",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "."
  },
  "containerEnv": {
    "PATH": "$PATH:/workspaces/alchemyql/.venv/bin"
  },
  "workspaceFolder": "/workspaces/alchemyql",
  "overrideCommand": false,
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "charliermarsh.ruff"
      ]
    }
  }
}
